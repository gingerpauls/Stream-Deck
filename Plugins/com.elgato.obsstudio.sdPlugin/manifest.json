		{
			"Platform": "windows",
			"MinimumVersion": "10"
		},
		{
			"Platform": "mac",
			"MinimumVersion": "10.11"
		}
	],
	"Software": {
		"MinimumVersion": "5.3"
	},
	"ApplicationsToMonitor": {
